@page
@model IdentityServer.Pages.Account.Logout.Index
@{
    ViewData[ViewDataKeys.Title] = "Logout";
}

<div class="logout-page">
    <div class="lead">
        <h1>Logout</h1>
        <p>Would you like to log out of IdentityServer?</p>
    </div>

    <form asp-page="@AccountPageConstants.Logout">
        <input type="hidden" id="logoutId" name="logoutId" value="@Model.LogoutId"  />
        
        <div class="mb-3">
            <button class="btn btn-primary">Yes</button>
        </div>
    </form>
</div>